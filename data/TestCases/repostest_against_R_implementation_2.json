    "Body": "\"\"\"\n    The following R-code was used:\n        \n    > install.packages(\"arules\")\n    > col1 = c(\"b\", \"b\", \"c\", \"b\", \"a\", \"a\", \"b\", \"c\", \"b\", \"b\", \"a\", \"b\", \"a\", \n    \"a\", \"a\", \"c\", \"b\", \"a\", \"b\", \"b\", \"b\", \"c\", \"a\", \"c\", \"a\", \"a\", \"c\", \"a\", \n    \"b\", \"b\", \"a\", \"c\") \n    > col2 = c(\"e\", \"f\", \"e\", \"e\", \"f\", \"e\", \"d\", \"f\", \"e\", \"e\", \"e\", \"d\", \"e\", \n    \"e\", \"f\", \"d\", \"d\", \"d\", \"e\", \"f\", \"f\", \"d\", \"d\", \"f\", \"e\", \"e\", \"f\", \"f\", \n    \"f\", \"d\", \"e\", \"e\") \n    > col3 = c(\"g\", \"i\", \"j\", \"i\", \"i\", \"j\", \"i\", \"h\", \"g\", \"j\", \"g\", \"h\", \"i\", \n    \"h\", \"g\", \"h\", \"g\", \"j\", \"h\", \"i\", \"g\", \"g\", \"i\", \"h\", \"h\", \"h\", \"h\", \"g\", \n    \"j\", \"i\", \"g\", \"g\")\n    > df = data.frame(col1, col2, col3)\n    > df <- data.frame(sapply(df, as.factor))\n    > rules <- apriori(df, parameter = list(supp = 0.2, conf = 0.2))\n    > inspect(head(rules, by = \"confidence\"))\n    \"\"\"\ntransactions = [('b', 'e', 'g'), ('b', 'f', 'i'), ('c', 'e', 'j'), ('b',\n    'e', 'i'), ('a', 'f', 'i'), ('a', 'e', 'j'), ('b', 'd', 'i'), ('c', 'f',\n    'h'), ('b', 'e', 'g'), ('b', 'e', 'j'), ('a', 'e', 'g'), ('b', 'd', 'h'\n    ), ('a', 'e', 'i'), ('a', 'e', 'h'), ('a', 'f', 'g'), ('c', 'd', 'h'),\n    ('b', 'd', 'g'), ('a', 'd', 'j'), ('b', 'e', 'h'), ('b', 'f', 'i'), (\n    'b', 'f', 'g'), ('c', 'd', 'g'), ('a', 'd', 'i'), ('c', 'f', 'h'), ('a',\n    'e', 'h'), ('a', 'e', 'h'), ('c', 'f', 'h'), ('a', 'f', 'g'), ('b', 'f',\n    'j'), ('b', 'd', 'i'), ('a', 'e', 'g'), ('c', 'e', 'g')]\nitemsets, rules = apriori(transactions, 0.2, 0.2)\nfor rule in rules:\n    if rule == Rule(('a',), ('e',)):\n        assert abs(rule.support - 0.21875) < 1e-06\n        assert abs(rule.confidence - 0.5833333) < 1e-06\n    if rule == Rule(('e',), ('a',)):\n        assert abs(rule.support - 0.21875) < 1e-06\n        assert abs(rule.confidence - 0.5) < 1e-06\n"
}