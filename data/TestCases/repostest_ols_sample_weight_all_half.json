    "Body": "self.model = OLS(solver='pinv', fit_intercept=True, est_stderr=True,\n    reg_method=None, alpha=0, l1_ratio=0, tol=0.0001, max_iter=100, coef=\n    None, stderr=None, dispersion=None)\nself.model.fit(self.X, self.Y, sample_weight=0.5)\nnp.testing.assert_array_almost_equal(self.model.coef, np.array(((-\n    3482258.63459582, 15.0618722713733, -0.035819179292591, -\n    2.02022980381683, -1.03322686717359, -0.0511041056535807, \n    1829.15146461355), (-3482258.63459582, 15.0618722713733, -\n    0.035819179292591, -2.02022980381683, -1.03322686717359, -\n    0.0511041056535807, 1829.15146461355))).reshape(2, -1), decimal=3)\nnp.testing.assert_array_almost_equal(old_div(self.model.stderr, np.sqrt(\n    old_div(9.0, self.data_longley.exog.shape[0]))), np.array(((\n    890420.383607373, 84.9149257747669, 0.0334910077722432, \n    0.488399681651699, 0.214274163161675, 0.22607320006937, \n    455.478499142212), (890420.383607373, 84.9149257747669, \n    0.0334910077722432, 0.488399681651699, 0.214274163161675, \n    0.22607320006937, 455.478499142212))).reshape(2, -1), decimal=1)\nnp.testing.assert_array_almost_equal(old_div(self.model.dispersion, old_div\n    (9.0, self.data_longley.exog.shape[0])), np.array(((92936.0061673238, \n    92936.0061673238), (92936.0061673238, 92936.0061673238))), decimal=3)\nres = np.array((267.34003, -94.01394, 46.28717, -410.11462, 309.71459, -\n    249.31122, -164.04896, -13.18036, 14.30477, 455.39409, -17.26893, -\n    39.05504, -155.54997, -85.67131, 341.93151, -206.75783)).reshape(-1, 1)\nnp.testing.assert_array_almost_equal(self.Y - self.model.predict(self.X),\n    np.hstack((res, res)), decimal=3)\nself.assertRaises(ValueError, self.model.loglike, self.X, self.Y)\n"
