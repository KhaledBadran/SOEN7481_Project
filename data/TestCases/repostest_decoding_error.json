    "Body": "encryptedContent = EncryptedContent()\nerrorBlob1 = Blob(bytearray([31, 48, 28, 22, 7, 20, 8, 4, 116, 101, 115, \n    116, 8, 3, 107, 101, 121, 8, 7, 108, 111, 99, 97, 116, 111, 114, 131, 1,\n    0, 133, 10, 114, 97, 110, 100, 111, 109, 98, 105, 116, 115, 132, 7, 99,\n    111, 110, 116, 101, 110, 116]), False)\nself.assertRaises(ValueError, lambda : encryptedContent.wireDecode(errorBlob1))\nerrorBlob2 = Blob(bytearray([130, 48, 29, 22, 7, 20, 8, 4, 116, 101, 115, \n    116, 8, 3, 107, 101, 121, 8, 7, 108, 111, 99, 97, 116, 111, 114, 131, 1,\n    0, 133, 10, 114, 97, 110, 100, 111, 109, 98, 105, 116, 115, 132, 7, 99,\n    111, 110, 116, 101, 110, 116]), False)\nself.assertRaises(ValueError, lambda : encryptedContent.wireDecode(errorBlob2))\nerrorBlob3 = Blob(bytearray([130, 48, 28, 22, 7, 20, 8, 4, 116, 101, 115, \n    116, 8, 3, 107, 101, 121, 8, 7, 108, 111, 99, 97, 116, 111, 114, 29, 1,\n    0, 133, 10, 114, 97, 110, 100, 111, 109, 98, 105, 116, 115, 132, 7, 99,\n    111, 110, 116, 101, 110, 116]), False)\nself.assertRaises(ValueError, lambda : encryptedContent.wireDecode(errorBlob3))\nerrorBlob4 = Blob(bytearray([130, 48, 28, 22, 7, 20, 8, 4, 116, 101, 115, \n    116, 8, 3, 107, 101, 121, 8, 7, 108, 111, 99, 97, 116, 111, 114, 131, 1,\n    0, 31, 10, 114, 97, 110, 100, 111, 109, 98, 105, 116, 115, 132, 7, 99, \n    111, 110, 116, 101, 110, 116]), False)\nself.assertRaises(ValueError, lambda : encryptedContent.wireDecode(errorBlob4))\nerrorBlob5 = Blob(bytearray([130, 48, 28, 22, 7, 20, 8, 4, 116, 101, 115, \n    116, 8, 3, 107, 101, 121, 8, 7, 108, 111, 99, 97, 116, 111, 114, 131, 1,\n    0, 133, 10, 114, 97, 110, 100, 111, 109, 98, 105, 116, 115, 33, 7, 99, \n    111, 110, 116, 101, 110, 116]), False)\nself.assertRaises(ValueError, lambda : encryptedContent.wireDecode(errorBlob5))\nerrorBlob6 = Blob(bytearray([130, 0]), False)\nself.assertRaises(ValueError, lambda : encryptedContent.wireDecode(errorBlob6))\n"
